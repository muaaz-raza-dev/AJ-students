
{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "headers": [
        {
          "source": "/_next/static/(.*)", 
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store, must-revalidate"
            }
          ]
        }
      ]
  }
  